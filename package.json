{
  "name": "svgc-loader",
  "version": "0.2.7",
  "description": "SVG component loader.",
  "type": "module",
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs",
    "types": "./types/index.d.ts"
  },
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/svgc-loader.git"
  },
  "keywords": [
    "svg",
    "svgo",
    "react",
    "loader",
    "webpack"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/svgc-loader/issues"
  },
  "homepage": "https://github.com/nuintun/svgc-loader#readme",
  "scripts": {
    "test": "node tests",
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write . --ignore-path .prettierignore",
    "build": "npm run clean && npm run build:main && npm run format",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir types && node tools/fix-types.js"
  },
  "dependencies": {
    "css-tree": "^2.3.1",
    "svgo": "^3.3.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/css-tree": "^2.3.7",
    "@types/node": "^20.12.11",
    "magic-string": "^0.30.10",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.6",
    "rollup": "^4.17.2",
    "tsc-alias": "^1.8.9",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  },
  "peerDependencies": {
    "preact": "*",
    "react": "*",
    "react-native-svg": "*",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  }
}

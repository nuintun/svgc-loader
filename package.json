{
  "name": "svgc-loader",
  "version": "0.1.3",
  "description": "SVG component loader.",
  "type": "module",
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs"
  },
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "scripts": {
    "test": "node tests",
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "build": "npm run clean && npm run build:main && npm run format",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/svgc-loader.git"
  },
  "keywords": [
    "svg",
    "svgo",
    "react",
    "loader",
    "webpack"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/svgc-loader/issues"
  },
  "homepage": "https://github.com/nuintun/svgc-loader#readme",
  "dependencies": {
    "css-tree": "^2.3.1",
    "svgo": "^3.0.2",
    "tslib": "^2.4.1"
  },
  "peerDependencies": {
    "preact": "*",
    "react": "*",
    "react-native-svg": "*"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/css-tree": "^2.0.1",
    "magic-string": "^0.27.0",
    "prettier": "^2.8.2",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  }
}
